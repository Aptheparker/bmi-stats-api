<html>
  <head>
    <title><%= pageTitle %></title>
    <link rel="stylesheet" href="/css/style.css" />
  </head>
  <body>
    <%- include('../partials/navbar') %>

    <main>
      <% for (let p of products) { %>
      <h2><%= p.productName %></h2>
      <p><%= p.productPrice %></p>
      <a href="/shop/<%= p.id %>">View Details</a>
      <% } %>
    </main>
  </body>
</html>
